
!-------------------------------------------------------------------------------
! bubbler configuration
!-------------------------------------------------------------------------------
!
!     logical               :: on   (bubbler on/off switch)
!     character(len=20)     :: data_file
!     REALTYPE              :: aflow
!     integer               :: nports
!     REALTYPE              :: bublen
!     REALTYPE              :: bdepth
!     logical               :: opopt
!     REALTYPE              :: ton
!     REALTYPE              :: toff
!     logical               :: intopt (plume interaction?)
!     character(len=20)     :: start
!     character(len=20)     :: stop
!     logical               :: eff
!     character(len=20)     :: eff_file
!
!-------------------------------------------------------------------------------
&bubbler
   on = .true.,
   data_file = 'bubbler.csv',
   aflow = 1.0,
   nports = 1,
   bublen = 1.0,
   bdepth = 1.0,
   opopt = .false.,
   ton = 1.0
   toff = 1.0
   intopt = .false.,
   start = '1998-05-01',
   stop = '2007-05-02',
   eff = .false.
   eff_file = 'bubleff.txt'
/
