#!/usr/bin/make -f

package=glm
year=2017
version=2.4.0dev02
release=0.0

#export DH_VERBOSE=1

binary:
	dh_testdir
	-mkdir -p $(CURDIR)/debian/glm/usr/local/bin
	-(cp $(CURDIR)/glm $(CURDIR)/debian/glm/usr/local/bin)
	touch build-stamp
	dh_compress
	dh_fixperms
	dh_installdeb
	dh_gencontrol
	dh_md5sums
	dh_builddeb


clean:
	dh_clean


.PHONY: clean binary
